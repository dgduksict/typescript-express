{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "migrate:development": "npx prisma migrate dev",
    "migrate:staging": "dotenv -e .env.staging -- npx prisma migrate deploy",
    "migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy",
    "seed:development": "nodemon src/seeders/main.ts",
    "seed:staging": "dotenv -e .env.staging -- nodemon src/seeders/main.ts",
    "seed:production": "dotenv -e .env.production -- nodemon src/seeders/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "duke",
  "license": "MIT",
  "description": "This is a boiler made by Duke.",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.919.0",
    "@aws-sdk/lib-storage": "^3.917.0",
    "@prisma/client": "^6.16.3",
    "axios": "^1.7.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "multer": "^2.0.2",
    "kysely": "^0.28.7",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.11.6",
    "axios-mock-adapter": "^2.1.0",
    "bcrypt": "^6.0.0",
    "ioredis": "^5.7.0",
    "jest": "^30.1.3",
    "prisma-kysely": "^2.2.1",
    "redis": "^5.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uploadthing": "^7.7.3"
  }
